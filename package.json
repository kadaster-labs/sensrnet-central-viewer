  "version": "0.6.6",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config src/proxy.conf.json --configuration=en",
    "start-nl": "npm run start -- --configuration=nl",
    "build": "ng build --configuration production",
    "test": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI --code-coverage",
